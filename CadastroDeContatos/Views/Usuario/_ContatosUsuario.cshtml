@model List<ContatoModel>

<table class="table" id="table-contatos-usuario">
	<thead>
		<tr>
			<th scope="col" class="text-center">#</th>
			<th scope="col" class="text-center">Nome</th>
			<th scope="col" class="text-center">Email</th>
			<th scope="col" class="text-center">Celular</th>
		</tr>
	</thead>
	<tbody class="table-group-divider">
		@if (Model != null && Model.Any())
		{
			foreach (var contato in Model)
			{
				<tr>
					<th scope="row" class="text-center">@contato.Id</th>
					<td class="text-center">@contato.Name</td>
					<td class="text-center">@contato.Email</td>
					<td class="text-center">@contato.Celular</td>
				</tr>
			}
		}
		else
		{
			<tr class="odd"><td valign="top" colspan="4" class="text-center">Nenhum contato encontrado</td></tr>
		}

	</tbody>
</table>